    "mysql2": "^2.1.0",
    "node-sass": "^4.12.0",
    "pg": "^7.12.1",
    "pg-hstore": "^2.3.3",
    "pluralize": "^8.0.0",
    "popper.js": "^1.15.0",
    "reflect-metadata": "^0.1.10",
    "sass-loader": "^7.1.0",
    "shortid": "^2.2.14",
