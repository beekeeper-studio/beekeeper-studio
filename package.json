{
  "name": "beekeeper-studio",
  "version": "1.6.11",
  "private": true,
  "description": "An easy-to use SQL query editor and database UI for Mac, Windows, and Linux",
  "author": {
    "name": "Beekeeper Studio Team",
    "email": "matthew@rathbonelabs.com",
    "url": "https://beekeeperstudio.io"
  },
  "scripts": {
    "serve": "vue-cli-service serve",
    "test:unit": "TEST_MODE=1 vue-cli-service test:unit",
    "lint": "vue-cli-service lint",
    "electron:build": "vue-cli-service electron:build",
    "electron:serve": "vue-cli-service electron:serve --host localhost",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "release:prs": "./build/get-prs.sh"
  },
  "main": "background.js",
  "dependencies": {
    "cassandra-driver": "^4.4.0",
    "cli-highlight": "2.0.0",
    "codemirror": "^5.48.2",
    "connection-string": "^3.4.2",
    "core-js": "3",
    "dateformat": "^3.0.3",
    "electron-log": "^4.2.2",
    "electron-util": "^0.14.1",
    "humanize-duration": "^3.23.1",
    "javascript-time-ago": "^2.0.8",
    "jquery": "^3.5.0",
    "knex": "^0.21.2",
    "lodash": "^4.17.19",
    "material-icons": "^0.3.1",
    "mssql": "^6.2.0",
    "mysql2": "^2.1.0",
    "node-sass": "^4.12.0",
    "node-ssh-forward": "^0.7.2",
    "pg": "^7.12.1",
    "pg-hstore": "^2.3.3",
    "pluralize": "^8.0.0",
    "popper.js": "^1.15.0",
    "portfinder": "^1.0.26",
    "reflect-metadata": "^0.1.10",
    "sass-loader": "^7.1.0",
    "shortid": "^2.2.14",
    "simple-encryptor": "^3.0.0",
    "split.js": "^1.5.11",
    "sql-formatter": "^2.3.3",
    "sql-query-identifier": "^1.1.0",
    "sqlite3": "^4.1.1",
    "ssh2": "beekeeper-studio/ssh2#electron-detection",
    "tabulator-tables": "^4.7.2",
    "typeface-roboto": "^0.0.75",
    "typeface-source-code-pro": "^1.1.3",
    "typeorm": "^0.2.25",
    "typeorm-encrypted-column": "^0.0.4",
    "username": "^5.1.0",
    "v-hotkey": "^0.8.0",
    "v-tooltip": "^2.0.2",
    "vue": "^2.6.10",
    "vue-clipboard2": "^0.3.1",
    "vue-js-modal": "^1.3.33",
    "vuejs-noty": "^0.1.3",
    "vuex": "^3.1.1",
    "vuex-persist": "^2.0.1",
    "xel": "beekeeper-studio/xel",
    "xlsx": "^0.16.2",
    "yup": "^0.27.0"
  },
  "devDependencies": {
    "@types/node": "^8.0.29",
    "@vue/cli-plugin-babel": "^4.1.2",
    "@vue/cli-plugin-eslint": "^4.1.2",
    "@vue/cli-plugin-unit-jest": "~4.4.0",
    "@vue/cli-service": "^4.1.2",
    "@vue/test-utils": "^1.0.3",
    "aws-sdk": "^2.645.0",
    "babel-eslint": "^10.0.1",
    "electron": "8.2.5",
    "electron-builder-notarize": "beekeeper-studio/electron-builder-notarize#master",
    "electron-rebuild": "^1.8.5",
    "electron-updater": "^4.2.5",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "vue-cli-plugin-electron-builder": "^2.0.0-rc.1",
    "vue-template-compiler": "^2.6.10",
    "vuepress": "^1.5.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-console": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint",
      "ecmaFeatures": {
        "legacyDecorators": true
      }
    },
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/tests/unit/**/*.spec.{j,t}s?(x)"
        ],
        "env": {
          "jest": true
        }
      }
    ]
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "resolutions": {
    "watchpack": "1.6.1",
    "pg/pgpass": "beekeeper-studio/pgpass#blank-home",
    "node-ssh-forward/ssh2": "beekeeper-studio/ssh2#electron-detection"
  }
}
