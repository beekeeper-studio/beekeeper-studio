    "vuex": "^3.1.1",
    "vuex-persist": "^2.0.1",
    "yup": "^0.27.0"
  },
  "devDependencies": {
    "@types/node": "^8.0.29",
    "@vue/cli-plugin-babel": "^4.1.2",
    "@vue/cli-plugin-eslint": "^4.1.2",
    "@vue/cli-service": "^4.1.2",
    "aws-sdk": "^2.645.0",
    "babel-eslint": "^10.0.1",
    "electron": "^6.0.0",
    "electron-builder-notarize": "beekeeper-studio/electron-builder-notarize#master",
